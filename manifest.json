{
  "manifest_version": 2,

  "name": "Blockify",
  "description": "Blocks those annoying alert/confirm dialogs, blocks onpageunload events",
  "version": "1.0.0",

  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "html/popup.html",
    "default_title": "Blockify"
  },
  "permissions": [ "http://*/*", "https://*/*", "tabs", "unlimitedStorage", "webRequest", "webRequestBlocking", "storage", "notifications" ]
}
